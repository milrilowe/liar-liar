#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/miles/dev/liar-liar/node_modules/.pnpm/precinct@12.2.0/node_modules/precinct/bin/node_modules:/home/miles/dev/liar-liar/node_modules/.pnpm/precinct@12.2.0/node_modules/precinct/node_modules:/home/miles/dev/liar-liar/node_modules/.pnpm/precinct@12.2.0/node_modules:/home/miles/dev/liar-liar/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/miles/dev/liar-liar/node_modules/.pnpm/precinct@12.2.0/node_modules/precinct/bin/node_modules:/home/miles/dev/liar-liar/node_modules/.pnpm/precinct@12.2.0/node_modules/precinct/node_modules:/home/miles/dev/liar-liar/node_modules/.pnpm/precinct@12.2.0/node_modules:/home/miles/dev/liar-liar/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../precinct/bin/cli.js" "$@"
else
  exec node  "$basedir/../precinct/bin/cli.js" "$@"
fi
