#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/miles/dev/liar-liar/node_modules/.pnpm/vitest@3.2.4_@types+node@24.3.0_jiti@2.5.1_jsdom@26.1.0_lightningcss@1.30.1_tsx@4.20.4_yaml@2.8.1/node_modules/vitest/node_modules:/home/miles/dev/liar-liar/node_modules/.pnpm/vitest@3.2.4_@types+node@24.3.0_jiti@2.5.1_jsdom@26.1.0_lightningcss@1.30.1_tsx@4.20.4_yaml@2.8.1/node_modules:/home/miles/dev/liar-liar/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/miles/dev/liar-liar/node_modules/.pnpm/vitest@3.2.4_@types+node@24.3.0_jiti@2.5.1_jsdom@26.1.0_lightningcss@1.30.1_tsx@4.20.4_yaml@2.8.1/node_modules/vitest/node_modules:/home/miles/dev/liar-liar/node_modules/.pnpm/vitest@3.2.4_@types+node@24.3.0_jiti@2.5.1_jsdom@26.1.0_lightningcss@1.30.1_tsx@4.20.4_yaml@2.8.1/node_modules:/home/miles/dev/liar-liar/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../vitest.mjs" "$@"
else
  exec node  "$basedir/../../vitest.mjs" "$@"
fi
